data(meta) ::= <<
{ "data":
  {
    "domains":
      [
$meta.SortedDomains:{domain|      "$domain.Name$"}; separator=",\n"$
      ],
    "interfaces":
      [
$meta.Interfaces:{interface|$interface(interface)$}; separator=",\n"$
      ],
    "classes":
      [
$meta.Classes:{class|$class(class)$}; separator=",\n"$
      ],
    "relationTypes":
      [
$meta.RelationTypes:{relationType|$relationType(relationType)$}; separator=",\n"$
      ],
    "methodTypes":
      [
$meta.MethodTypes:{methodType|$methodType(methodType)$}; separator=",\n"$
      ]
  }
}
>>

interface(interface) ::= <<
      {
        "id": "$interface.IdAsString$",
        "origin": "$interface.Origin$",
        "name": "$interface.Name$",
        "plural": "$interface.PluralName$"$if(interface.Supertypes)$,
        "interfaceIds":
          [
$interface.Supertypes:{interface|            "$interface.IdAsString$"}; separator=",\n"$
          ]
$endif$
      }
>>

class(class) ::= <<
      {
        "id": "$class.IdAsString$",
        "origin": "$class.Origin$",
        "name": "$class.Name$",
        "plural": "$class.PluralName$"$if(class.Supertypes)$,
        "interfaceIds":
