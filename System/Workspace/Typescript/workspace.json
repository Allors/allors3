{
  "version": 2,
  "projects": {
    "workspace-system": {
      "root": "libs/workspace/system",
      "sourceRoot": "libs/workspace/system/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/workspace/system/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/workspace/system"],
          "options": {
            "jestConfig": "libs/workspace/system/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "meta-lazy": {
      "root": "libs/meta/lazy",
      "sourceRoot": "libs/meta/lazy/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/meta/lazy/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/meta/lazy"],
          "options": {
            "jestConfig": "libs/meta/lazy/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tests-meta-lazy": {
      "root": "libs/tests/meta/lazy",
      "sourceRoot": "libs/tests/meta/lazy/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/tests/meta/lazy/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tests/meta/lazy"],
          "options": {
            "jestConfig": "libs/tests/meta/lazy/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
