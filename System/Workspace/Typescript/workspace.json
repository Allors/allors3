{
  "version": 2,
  "projects": {
    "workspace-system": {
      "root": "libs/workspace/system",
      "sourceRoot": "libs/workspace/system/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/workspace/system/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/workspace/system"],
          "options": {
            "jestConfig": "libs/workspace/system/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "workspace-lazy": {
      "root": "libs/workspace/lazy",
      "sourceRoot": "libs/workspace/lazy/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/workspace/lazy/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/workspace/lazy"],
          "options": {
            "jestConfig": "libs/workspace/lazy/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tests-workspace-lazy": {
      "root": "libs/tests/workspace/lazy",
      "sourceRoot": "libs/tests/workspace/lazy/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/tests/workspace/lazy/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tests/workspace/lazy"],
          "options": {
            "jestConfig": "libs/tests/workspace/lazy/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": { "defaultCollection": "@nrwl/workspace" }
}
